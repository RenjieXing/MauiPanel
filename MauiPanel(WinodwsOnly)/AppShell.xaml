<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiPanel.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiPanel"
    xmlns:views="clr-namespace:MauiPanel.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiPanel">
    <FlyoutItem>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        <ShellContent
        Title="UnsafeAccessor"
        ContentTemplate="{DataTemplate views:UnsafeAccessorView}"
        Route="UnsafeAccessorView" />
        <ShellContent
        Title="Unsafe"
        ContentTemplate="{DataTemplate views:UnsafeView}"
        Route="UnsafeView" />
    </FlyoutItem>
</Shell>
